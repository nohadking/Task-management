  <script>
    $(document).ready(function () {
        $('.category-filter').change(function () {
            console.log("category-filter changed");
            var selectedCategories = [];

            $('.category-filter:checked').each(function () {
                selectedCategories.push($(this).val());
            });

            if (selectedCategories.length === 0) {
                console.warn("No categories selected.");
                return; // لا ترسل الطلب إذا لم يتم تحديد أي فئة
            }

            var url = `${window.origin}/PublicProductsAPI/GetProductsByCategoyyName`;

            fetch(url, {
                method: "POST",
                headers: {
                    "Content-Type": "application/json"
                },
                body: JSON.stringify(selectedCategories)
            })
                .then(response => {
                    if (!response.ok) {
                        throw new Error("Network response was not ok");
                    }
                    return response.json();
                })
                .then(data => {
                    console.log("Products:", JSON.stringify(data));

                    const container = document.getElementById("margin-res");
                    container.innerHTML = "";

                    data.forEach(item => {
                        // ✅ التصحيح هنا بوضع النص بين علامات الاقتباس `
                        const productUrl = `/Product_page/MYProduct?ProductId=${encodeURIComponent(item.idProduct)}`;

                        const productHtml = `
                            <div class="col-xl-3 col-6 col-grid-box">
                                <div class="product-box">
                                    <div class="img-wrapper">
                                        <div class="front">
                                            <a href="${productUrl}">
                                                <img src="/Images/Home/${item.photo}" class="img-fluid blur-up lazyload bg-img" alt="" style="height:150px;width:150px">
                                            </a>
                                        </div>
                                        <div class="back">
                                            <a href="${productUrl}">
                                                <img src="/Images/Home/${item.photo}" class="img-fluid blur-up lazyload bg-img" alt="">
                                            </a>
                                        </div>
                                    </div>
                                    <div class="product-detail">
                                        <a href="${productUrl}">
                                            <h6>${item.productNameEn}</h6>
                                        </a>
                                        <p>${item.descriptionEn}</p>
                                        <h4>$${item.price}</h4>
                                    </div>
                                </div>
                            </div>
                        `;

                        container.innerHTML += productHtml;
                    });
                })
                .catch(error => {
                    console.error("There was a problem with the fetch operation:", error);
                });
        });
    });
</script>


