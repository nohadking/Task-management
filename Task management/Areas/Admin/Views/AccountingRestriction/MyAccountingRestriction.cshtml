@model ViewmMODeElMASTER
@{
    ViewData["Title"] = ResourceWeb.LBMyAccountingRestriction;
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="page-wrapper">
    <div class="content">
        <div class="page-header">
            <div class="add-item d-flex">
                <div class="page-title">
                    <h4>AccountingRestriction</h4>
                    <h6>Manage your categories</h6>
                </div>
            </div>
            <ul class="table-top-head">
                <li>
                    <a onclick="CreatePdf()" data-bs-toggle="tooltip" data-bs-placement="top" title="Pdf"><img src="~/AdminEn/assets/img/icons/pdf.svg" alt="img"></a>
                </li>
                <li>
                    <a data-bs-toggle="tooltip" data-bs-placement="top" title="Excel"><img src="~/AdminEn/assets/img/icons/excel.svg" alt="img"></a>
                </li>
                <li>
                    <a onclick="printDirectPdf()" data-bs-toggle="tooltip" data-bs-placement="top" title="Print"><i data-feather="printer" class="feather-rotate-ccw"></i></a>
                </li>
                <li>
                    <a data-bs-toggle="tooltip" data-bs-placement="top" title="Refresh"><i data-feather="rotate-ccw" class="feather-rotate-ccw"></i></a>
                </li>
                <li>
                    <a data-bs-toggle="tooltip" data-bs-placement="top" title="Collapse" id="collapse-header"><i data-feather="chevron-up" class="feather-chevron-up"></i></a>
                </li>
            </ul>
            <div class="page-btn">
                <a href="#" class="btn btn-added" data-bs-toggle="modal" data-bs-target="#add-AccountingRestriction"><i data-feather="plus-circle" class="me-2"></i>Add New AccountingRestriction</a>
            </div>
        </div>
        <!-- /product list -->
        <div class="card table-list-card">
            <div class="card-body">
                <div class="table-top">
                    <div class="search-set">
                        <div class="search-input">
                            <a href="" class="btn btn-searchset"><i data-feather="search" class="feather-search"></i></a>
                        </div>
                    </div>
                    <div class="search-path">
                        <a class="btn btn-filter" id="filter_search">
                            <i data-feather="filter" class="filter-icon"></i>
                            <span><img src="~/AdminEn/assets/img/icons/closes.svg" alt="img"></span>
                        </a>
                    </div>
                    <div class="form-sort">
                        <label>ختر تاريخ محدد  </label>
                        <select id="SelectDetectedDtForPrint" class="form-select" asp-for="@Model.AccountingRestriction.DateTimeEntry" asp-items="@(new SelectList(ViewBag.AccountingRestriction, "IdaccountingRestrictions", "DateTimeEntry"))">
                            <option></option>
                        </select>
                    </div>
                </div>
                <!-- /Filter -->
                <div class="card" id="filter_inputs">
                    <div class="card-body pb-0">
                        <div class="row">
                            <div class="col-lg-3 col-sm-6 col-12">
                                <div class="input-blocks">
                                    <label>حساب المورد</label>
                                    <select id="SelectSupAccForPrint" class="form-select" asp-for="@Model.AccountingRestriction.AccountingName" asp-items="@(new SelectList(ViewBag.AccountingRestriction, "IdaccountingRestrictions", "AccountingName"))">
                                        <option></option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-lg-3 col-sm-6 col-12">
                                <div class="input-blocks">
                                    <label>من تاريخ</label>

                                    <div class="input-groupicon">
                                        <input id="startDtForPrint" type="date" class="form-control" placeholder="من تاريخ">
                                    </div>
                                </div>
                            </div> 
                            <div class="col-lg-3 col-sm-6 col-12">
                                <div class="input-blocks">
                                    <label>الى تاريخ</label>

                                    <div class="input-groupicon">
                                        <input id="endDtForPrint" type="date" class="form-control" placeholder="إلى تاريخ ">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-sm-6 col-12">
                                <div class="input-blocks">
                                  @*   <i data-feather="stop-circle" class="info-img"></i> *@
                                    <label>نوع السند</label>
                                    <select id="SelectTypePurForPrint" class="form-select" asp-for="@Model.AccountingRestriction.BondType" asp-items="@(new SelectList(ViewBag.AccountingRestriction, "IdaccountingRestrictions", "BondType"))">
                                        <option></option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-lg-3 col-sm-6 col-12 ms-auto">
                                <div class="input-blocks">
                                    <a class="btn btn-filters ms-auto"> <i data-feather="search" class="feather-search"></i> Search </a>
                                </div>
                            </div>
                            <div class="page-title">
                                <span style="color:royalblue"> <strong>@TempData["Saved successfullyl"]</strong></span>
                                <span style="color:red"> <strong>@TempData["ErrorSave"]</strong></span>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /Filter -->
                <div class="table-responsive">
                    <table id="TblForPrintTBL" class="table  datanew">
                        <thead>
                            <tr>
                                <th class="no-sort">
                                    <label class="checkboxs">
                                        <input type="checkbox" id="select-all">
                                        <span class="checkmarks"></span>
                                    </label>
                                </th>
                                <th>@ResourceWeb.lbNumberaccountingRestrictions</th>
                                <th>@ResourceWeb.lbAccountingName</th>
                                <th>@ResourceWeb.lbBondType</th>
                                <th>@ResourceWeb.lbBondNumber</th>
                                <th>@ResourceWeb.lbDebtor</th>
                                <th>@ResourceWeb.lbcreditor</th>
                                <th>@ResourceWeb.lbStatement</th>
                                <th>@ResourceWeb.LbNouts</th>
                                <th>@ResourceWeb.LbDateTimeEntry</th>
                                <th>@ResourceWeb.LbDataEntry</th>
                                <th>@ResourceWeb.lbCurrentState</th>             
                                <th class="no-sort">@ResourceWeb.LbOpration</th>
                            </tr>
                        </thead>
                        <tbody id="TBLToPrint">
                            @foreach (var item in Model.ListAccountingRestriction)
                            {
                                <tr>
                                    <td>
                                        <label class="checkboxs">
                                            <input type="checkbox">
                                            <span class="checkmarks"></span>
                                        </label>
                                    </td>
                                    <td>@item.NumberaccountingRestrictions</td>
                                    <td>@item.AccountingName</td>
                                    <td>@item.BondType</td>
                                    <td>@item.BondNumber</td>
                                    <td>@item.Debtor</td>
                                    <td>@item.creditor</td>
                                    <td>@item.Statement</td>
                                    <td>@item.Nouts</td>
                                    <td>@item.DateTimeEntry</td>
                                    <td>@item.DataEntry</td>
                                    <td>@item.CurrentState</td>                            
                                    <td class="action-table-data">
                                        <a class="me-2 p-2 mb-0" asp-area="Admin" asp-controller="AccountingRestriction" asp-action="AddAccountingRestriction" asp-route-IdaccountingRestrictions="@item.IdaccountingRestrictions" title="تعديل ">
                                            <i data-feather="zap" class="feather-eye"></i>
                                        </a>
                                        <a class="confirm-text p-2" asp-area="Admin" asp-controller="AccountingRestriction" asp-action="DeleteData" asp-route-IdaccountingRestrictions="@item.IdaccountingRestrictions">
                                            <i data-feather="trash-2" class="feather-trash-2"></i>
                                        </a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- /product list -->
    </div>
</div>
<!-- /Main Wrapper -->
<!-- Add AccountingRestriction -->
<div class="modal fade" id="add-AccountingRestriction">
    <div class="modal-dialog modal-dialog-centered custom-modal-two">
        <div class="modal-content">
            <div class="page-wrapper-new p-0">
                <div class="content">
                    <div class="modal-header border-0 custom-modal-header">
                        <div class="page-title">
                            <h4>Create AccountingRestriction</h4>
                        </div>
                        <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body custom-modal-body">
                        <form role="form" method="post" asp-controller="AccountingRestriction" asp-action="Save" enctype="multipart/form-data">
                            <div class="form-group">
                                <input type="hidden" asp-for="@Model.AccountingRestriction.DataEntry" class="form-control" value="@User.Identity.Name">
                                <input type="hidden" asp-for="@Model.AccountingRestriction.IdaccountingRestrictions" class="form-control">
                                <input type="hidden" asp-for="@Model.AccountingRestriction.DateTimeEntry" value="@DateTime.Now">
                                <input type="hidden" asp-for="@Model.returnUrl" id="url">
                                <span style="color:royalblue"> <strong>@TempData["Saved successfully"]</strong></span>
                                <span style="color:red"> <strong>@TempData["ErrorSave"]</strong></span>
                            </div>
                            <div class="mb-3">
                                <label class="form-label" asp-for="@Model.AccountingRestriction.NumberaccountingRestrictions">@ResourceWeb.lbNumberaccountingRestrictions</label>
                                <input type="text" class="form-control" asp-for="@Model.AccountingRestriction.NumberaccountingRestrictions" value="@ViewBag.nomberMax">
                                <span asp-validation-for="@Model.AccountingRestriction.NumberaccountingRestrictions" class="text-danger"></span>
                             
                            </div>
                            <div class="mb-3">
                                <label class="form-label" asp-for="@Model.AccountingRestriction.AccountingName">@ResourceWeb.lbAccountingName</label>
                                <select id="mainAccountSelect" class="form-select" asp-for="@Model.AccountingRestriction.AccountingName"
                                        asp-items="@(new SelectList(ViewBag.LevelForeAccount, "LevelForeAccountsName", "LevelForeAccountsName"))">
                                    <option value="">اختر حساب</option>
                                </select>
                                <span asp-validation-for="@Model.AccountingRestriction.AccountingName" class="text-danger"></span>                            
                            </div> 
                            <div class="mb-3">
                                <label class="form-label" asp-for="@Model.AccountingRestriction.BondType">@ResourceWeb.lbBondType</label>
                                <input type="text" class="form-control" asp-for="@Model.AccountingRestriction.BondType">
                                <span asp-validation-for="@Model.AccountingRestriction.BondType" class="text-danger"></span>
                            </div> 
                            <div class="mb-3">
                                <label class="form-label" asp-for="@Model.AccountingRestriction.BondNumber">@ResourceWeb.lbBondNumber</label>
                                <input type="text" class="form-control" asp-for="@Model.AccountingRestriction.BondNumber">
                                <span asp-validation-for="@Model.AccountingRestriction.BondNumber" class="text-danger"></span>
                            </div>

                            <div class="mb-3">
                                <label class="form-label" asp-for="@Model.AccountingRestriction.Debtor">@ResourceWeb.lbDebtor</label>
                                <input type="text" class="form-control" asp-for="@Model.AccountingRestriction.Debtor">
                                <span asp-validation-for="@Model.AccountingRestriction.Debtor" class="text-danger"></span>
                            </div> 
                            <div class="mb-3">
                                <label class="form-label" asp-for="@Model.AccountingRestriction.creditor">@ResourceWeb.lbcreditor</label>
                                <input type="text" class="form-control" asp-for="@Model.AccountingRestriction.creditor">
                                <span asp-validation-for="@Model.AccountingRestriction.creditor" class="text-danger"></span>
                            </div>  
                            <div class="mb-3">
                                <label class="form-label" asp-for="@Model.AccountingRestriction.Statement">@ResourceWeb.lbStatement</label>
                                <input type="text" class="form-control" asp-for="@Model.AccountingRestriction.Statement">
                                <span asp-validation-for="@Model.AccountingRestriction.Statement" class="text-danger"></span>
                            </div> 
                            <div class="mb-3">
                                <label class="form-label" asp-for="@Model.AccountingRestriction.Nouts">@ResourceWeb.Lbnotes</label>
                                <input type="text" class="form-control" asp-for="@Model.AccountingRestriction.Nouts">
                                <span asp-validation-for="@Model.AccountingRestriction.Nouts" class="text-danger"></span>
                            </div>
                            <div class="mb-0">
                                <div class="status-toggle modal-status d-flex justify-content-between align-items-center">
                                    <span class="status-label">Status</span>
                                    <input type="checkbox" id="user2" asp-for="@Model.AccountingRestriction.CurrentState" class="check" checked="">
                                    <label for="user2" class="checktoggle"></label>
                                </div>
                            </div>                   
                            <div class="modal-footer-btn">
                                <button type="button" class="btn btn-cancel me-2" data-bs-dismiss="modal">Cancel</button>
                                <button type="submit" class="btn btn-submit">Create AccountingRestriction</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /Add AccountingRestriction -->
