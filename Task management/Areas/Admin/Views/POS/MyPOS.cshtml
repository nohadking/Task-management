    <script>
        function SaveInvoiceInDB() {
            const idCustomerCategoryy = document.querySelector('#idForUCustCate').value;
            const invoieNumberr = parseInt(document.querySelector('#IIdInvoiceNumberForSave').textContent.trim(), 10);
            const idPaymentMethodd = parseInt(document.querySelector('#idPayMeth1')?.textContent?.trim(), 10) || null;
            const idForInvHed = parseInt(0, 10);
            const flexSwitchCheckChecked = document.querySelector("#flexSwitchCheckChecked").checked;
            const carintstaite = document.querySelector("#carintstaite").checked;
            const UserIdentity = document.querySelector("#UserIdentity").value;
            const timeNow = new Date().toISOString();
            const payload = {
                "idInvoseHeder": idForInvHed,
                "invoiceNumber": invoieNumberr,
                "dateInvos": new Date().toISOString(),
                "idUser": idCustomerCategoryy.toString(),
                "dataEntry": UserIdentity,
                "idPaymentMethod": idPaymentMethodd,
                "dateTimeEntry": new Date().toISOString(),
                "outstandingBill": flexSwitchCheckChecked,
                "currentState": carintstaite
            }

            console.log("payload " + JSON.stringify(

                payload
            ))
            const baseUrl = `${window.location.origin}/api`;
            fetch(`${baseUrl}/InvoiceHeader`, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify(
                    
                    payload
                )
            })
                .then(response => {
                    return response.json();
                })
                .then(data => {
                    console.log('Response Data (InvoiceHeader):', data);

                    document.querySelector("#InvoiceNumberMaimAlMain").textContent = data.invoiceNumber.toString();
                    //معلومات المنتجات
                    const addedProducts = document.querySelectorAll("#productsAddedToOrder .product-list");
                    const productRequests = [];
                    addedProducts.forEach(product => {
                        const productName = product.querySelector(".product-info h6 a").textContent.trim();
                        const idPro = parseInt(product.querySelector(".iiddPro").textContent.trim(), 10);
                        const price = parseFloat(
                            product.querySelector(".product-info .info h5").textContent.trim().replace(/[^0-9.]/g, '')
                        );
                        const qty = parseInt(product.querySelector('input[name="qty"]').value);

                        const p1 = {
                            "idInvose": 0,
                            "idInvoseHeder": parseInt(data.idInvoseHeder, 10),
                            "idProduct": idPro,
                            "quantity": qty,
                            "price": price,
                            "total": parseFloat(price, 10) * qty,
                            "dataEntry": UserIdentity,
                            "dateTimeEntry": new Date().toISOString(),
                            "currentState": true
                        }

                        console.log("p1  " + JSON.stringify(p1))
                        // إضافة طلب إرسال المنتج إلى المصفوفة
                        productRequests.push(
                            fetch(`${baseUrl}/Invoice`, {
                                method: 'POST',
                                headers: {
                                    'Content-Type': 'application/json',
                                },
                                body: JSON.stringify(p1)
                            })
                                .then(response => {
                                    return response.json();
                                })
                                .then(productData => {
                                    console.log('Response Data (InvoiceDetails):', productData);
                                })
                                .catch(error => {
                                    console.error('Error saving product:', error);
                                })
                        );
                    });

                    return Promise.all(productRequests);
                })
                .then(() => {
                    console.log('All products saved successfully!');

                    const invUnm = document.querySelector("#InvoiceNumberMaimAlMain").textContent;

                    console.log("invUnm :" + invUnm)

                    const url = `${baseUrl}/Invoice/${invUnm}`;

                    fetch(url, {
                        method: 'GET',
                        headers: {
                            'Content-Type': 'application/json',
                        },
                    })
                        .then(response => {
                            if (!response.ok) {
                                throw new Error(`HTTP error! status: ${response.status}`);
                            }
                            return response.json();
                        })
                        .then(data => {

                            console.log("INV :", data )

                            document.querySelector("#InvoiceNoToPrint").textContent = data[0].invoiceNumber;
                            document.querySelector("#CustomerIdToPrint").textContent = data[0].name;
                            let rawDate = data[0].dateInvos;

                            // تحويل النص إلى كائن تاريخ
                            let dateObj = new Date(rawDate);

                            // استخراج القيم المطلوبة
                            let day = dateObj.getDate().toString().padStart(2, '0');
                            let month = (dateObj.getMonth() + 1).toString().padStart(2, '0'); // الأشهر تبدأ من 0
                            let year = dateObj.getFullYear();
                            let hours = dateObj.getHours().toString().padStart(2, '0');
                            let minutes = dateObj.getMinutes().toString().padStart(2, '0');
                            // تنسيق التاريخ بالشكل المطلوب
                            let formattedDate = `${day}-${month}-${year} ${hours}:${minutes}`;
                            // عرض التاريخ في العنصر
                            document.querySelector("#DateInvToPrint").textContent = formattedDate;
                            // document.querySelector("#DateInvToPrint").textContent = data[0].dateInvos;
                            let tableBody = document.querySelector("#TBForRecipt");
                            tableBody.innerHTML = "";
                            var tot = 0;
                            var qtyy = 0;
                            data.forEach(item => {
                                let row = document.createElement("tr");

                                let nameCell = document.createElement("td");
                                nameCell.textContent = item.productNameAr;

                                let priceCell = document.createElement("td");
                                priceCell.textContent = item.price;

                                let quantityCell = document.createElement("td");
                                quantityCell.textContent = item.quantity;

                                let totalCell = document.createElement("td");
                                totalCell.textContent = parseFloat(item.price) * parseInt(item.quantity);

                                tot =  tot + item.total;
                                qtyy = qtyy + item.quantity;

                                row.appendChild(nameCell);
                                row.appendChild(priceCell);
                                row.appendChild(quantityCell);
                                row.appendChild(totalCell);


                                tableBody.appendChild(row);
                            });

                            console.log("tot " +tot) ;
                            console.log("qtyy"  +qtyy) ;

                            document.querySelector("#totalPriceForAll").textContent = tot.toFixed(3);
                            document.querySelector("#totalQTYForAll").textContent = qtyy.toString();
                        })
                        .catch(error => {
                            console.error('حدث خطأ أثناء الحذف:', error);
                        });

                })
                .catch(error => {
                    console.error('Error:', error);
                });
        }
